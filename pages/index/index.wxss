/**index.wxss**/
page {
  background-color: #F8F8F8;
  width: 100%;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}
.img image{
  width: 36px;
  height: 36px;
  position:absolute;
  right:25px; 
  z-index: 9999;
}
.rotateAn{
  animation: rotate 3s linear infinite;
}
.animation-view{

   margin: 30px auto;
   width:20%;
}
@keyframes rotate{
  from{ transform: rotate(0deg)}
  to{transform: rotate(360deg)}
}

.scr{
  width: 100%;
  height: calc(100% + 50rpx);
  position: absolute;
  top: -50rpx;
  bottom: 0;
  left: 0;
}
.top{
  width: 100%;
  height: calc(100% + 50rpx);
  /* background: #ccc; */
  /*position: relative;
   margin: 100rpx auto;
  padding: 0;
  transform: scale(1); */
}
.middle{
  width: 100%;
  height: auto;
  font-size: 32rpx;
  position: absolute;
  margin: 40% auto;
  transform: scale(1);
}
.user{
  width:200rpx;
  height: 200rpx;
  top:150rpx;
  left:300rpx;
  position: absolute;
  margin: 0;
  /* border: 1px solid #ccc;  */
}
.user image{
  height: 160rpx;
  width: 160rpx;
  display: block;
  margin: 0 auto;
  animation: fadeIn 3s 1 0s;
}
@keyframes fadeIn{
  0%{
    opacity: 0;
  }
  100%{
    opacity: 1;
  }
}
.user text{
  display: block;
  width:200rpx;
  text-align: center;
}
.txt{
  left:120rpx;
  top:120rpx;
  height:120rpx;
  width:120rpx;
  color:#fff;
  background:rgb(166, 138, 56);
  border-radius:50%;
  position:absolute;
  transform-origin:270rpx 130rpx;
  animation:rotate 10s infinite linear;
}

.txtorbit{
  left: 160rpx;
  top: 20rpx;
  width: 450rpx;
  height: 450rpx;
  border: 1px dashed gray;
  border-radius: 50%;
  position: absolute;
  margin: 0;
  padding: 0;
}

.txt2{
  left:280rpx;
  top: -30rpx;
  height:120rpx;
  width:120rpx;
  color:#fff;
  background:rgb(8, 189, 78);
  border-radius:50%;
  position:absolute;
  transform-origin:100rpx 270rpx;
  animation:rotate 10s infinite linear;
}

.txt3{
  left:540rpx;
  top: 220rpx;
  height:120rpx;
  width:120rpx;
  color:#fff;
  background:rgb(64, 124, 255);
  border-radius:50%;
  position:absolute;
  transform-origin:-160rpx 18rpx;
  animation:rotate 10s infinite linear;
}

.txt4{
  left:250rpx;
  top: 380rpx;
  height:120rpx;
  width:120rpx;
  color:#fff;
  background:rgb(255, 199, 15);
  border-radius:50%;
  position:absolute;
  transform-origin:140rpx -140rpx;
  animation:rotate 10s infinite linear;
}

.txtcon{
  display:block;
  margin:35rpx auto;
  text-align: center;
}
@keyframes rotate{
  100% {
    transform: rotate(-360deg);
  }
}
.bottom{
  display: flex;
  justify-content: space-between;
}
.wechat{
  width: 50%;
  display:block;
  line-height:80rpx;
  text-align: center;
  background:rgb(28, 226, 45);
}
.phone{
  width: 50%;
  display:block;
  line-height:80rpx;
  text-align: center;
  background: rgb(255, 232, 24);
}

/*  */
.two,.three,.four,.five{
  width: 100%;
  height: 100%;
  position: relative;
}
.tw_title,.th_title,.fo_title,.fi_title{
  position: relative;
  width: 300rpx;
  height: 80rpx;
  font-size: 32rpx;
  border: 1px solid #555;
  top: 20rpx;
  text-align: center;
  padding: 16rpx;
  animation: rote 3s 2s forwards;
  /* margin: 120rpx auto 40rpx auto; */
}

@keyframes rote{
  from{
    margin-left: -200rpx;
  }
  to{
    margin-left: 220rpx;
  }
}
.title{
  text-align: center;
  padding: 16rpx;
}
.tw_cont{
  border: 1px solid #555;
  margin: 70rpx auto 0;
  padding: 15rpx;
  animation: scale2 3s 3s forwards;
}
@keyframes scale2{
  from{width: 0;height: 0;}
  to{ width: 85%; height: auto;}
}
.tw_cont view{
  width: 95%;
  line-height: 45rpx;
  padding: 15rpx;
  border-bottom: 1px dotted #eee;
}

