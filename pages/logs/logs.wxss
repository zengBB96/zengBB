.two{
  width: 100%;
  height: 100%;
  /* background: rgba(214, 214, 214, 0.178); */
  position: relative;
}
.tw_title,.th_title,.fo_title{
  position: relative;
  width: 300rpx;
  height: 80rpx;
  font-size: 32rpx;
  border: 1px solid #555;
  top: 50rpx;
  text-align: center;
  padding: 16rpx;
  animation: rote 3s 2s forwards;
  /* margin: 120rpx auto 40rpx auto; */
}

@keyframes rote{
  from{
    margin-left: -200rpx;
  }
  to{
    margin-left: 220rpx;
  }
}
.title{
  text-align: center;
  padding: 16rpx;
}
.tw_cont{
  border: 1px solid #555;
  margin: 120rpx auto 120rpx auto;
  padding: 15rpx;
  animation: scale2 3s 3s forwards;
}
@keyframes scale2{
  from{width: 0;height: 0;}
  to{ width: 85%; height: auto;}
}
.tw_cont view{
  width: 95%;
  line-height: 45rpx;
  padding: 15rpx;
  border-bottom: 1px dotted #eee;
}

